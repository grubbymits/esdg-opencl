extern int BufferArg_0;
extern int BufferArg_1;
extern int BufferArg_2;
extern int BufferArg_3;
extern int BufferArg_4;
extern int BufferArg_5;
int main(void) {
  int id = 0;
  int num_cores = 2;
  int total_workgroups = 16;
  int workgroupX = 16;
  int workgroupY = 0;
  int x = 0;
  int y = 0;
  id = __builtin_le1_read_cpuid();

  while (id < total_workgroups) {
    x = id;
    if (x >= workgroupX) {
      y = x / workgroupX;
      x = x % workgroupX;
    }
    if (y > workgroupY)
      return 0;

    __builtin_le1_set_group_id_1(y);
    __builtin_le1_set_group_id_0(x);
    BFS_1(&BufferArg_0, &BufferArg_1, &BufferArg_2, &BufferArg_3, &BufferArg_4, &BufferArg_5, 4096);
    id += num_cores;
  }
  return id;
}

