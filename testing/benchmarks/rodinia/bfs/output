int main(void) {
  int id = 0;
  int num_cores = 1;
  int total_workgroups = 16;
  int workgroupX = 16;
  int workgroupY = 0;
  int x = 0;
  int y = 0;
  id = __builtin_le1_read_cpuid();

  while (id < total_workgroups) {
    x = id;
    if (x >= workgroupX) {
      y = x / workgroupX;
      x = x % workgroupX;
    }
