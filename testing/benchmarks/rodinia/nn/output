int main(void) {
  int id = 0;
  int num_cores = 8;
  int total_workgroups = 8;
  int workgroupX = 8;
  int workgroupY = 0;
  int x = 0;
  int y = 0;
  id = __builtin_le1_read_cpuid();

  while (id < total_workgroups) {
    x = id;
    if (x >= workgroupX) {
      y = x / workgroupX;
      x = x % workgroupX;
    }
    if (y > workgroupY)
      return 0;

    __builtin_le1_set_group_id_1(y);
    __builtin_le1_set_group_id_0(x);
    NearestNeighbor(&BufferArg_0, &BufferArg_1, 42764, 30, 90);
    id += num_cores;
  }
  return id;
}

