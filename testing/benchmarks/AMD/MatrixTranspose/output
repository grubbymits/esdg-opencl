  int id = 0;
  int num_cores = 8;
  int total_workgroups = 4;
  int workgroupX = 2;
  int workgroupY = 2;
  int x = 0;
  int y = 0;
  id = __builtin_le1_read_cpuid();

  while (id < total_workgroups) {
    x = id;
    if (x >= workgroupX) {
