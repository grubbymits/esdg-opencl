    __builtin_le1_set_group_id_0(x);
    matrixTranspose(&BufferArg_0, &BufferArg_1, (&BufferArg_2 + (__builtin_le1_read_cpuid() * 4096)));
    id += num_cores;
  }
  return id;
}

