#!/bin/sh

# Directories:
# - rodinia
#   - gaussian
#   - lud
#   - nn

# Each directory will contain a script to iterate through all the devices
declare -a benchmarks=(AMD/BinarySearch AMD/StringSearch AMD/BitonicSort AMD/FloydWarshall'rodinia/bfs rodinia/nw shoc/sort');

for benchmark in ${benchmarks[@]}
do
  (
    cd $benchmark
    make clean
    make
    bash ./run > output &
  )
done
